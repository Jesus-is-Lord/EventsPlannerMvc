@model IEnumerable<EventsPlannerMvc.Models.Vote>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Idea1.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VoteCode1.Code)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Idea1.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VoteCode1.Code)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
